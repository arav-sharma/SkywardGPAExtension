{
  "manifest_version": 3,
  "name": "Skyward GPA Calculator",
  "version": "1.0.0",
  "description": "Calculate your GPA easily! Log into Skyward & navigate to the gradebook.",
  "icons": {
    "32": "icon32.png",
    "512": "icon.png"
  },
  "permissions": [
    "https://skyward.iscorp.com/scripts/wsisa.dll/WService=wseduallenisdtx/sfgradebook001.w"
  ],
  "background": {
    "service_worker": "content.js"
  },
  "browser_action": {
    "default_icon": "icon32.png",
    "default_title": "Skyward GPA Calculator"
  }
}
